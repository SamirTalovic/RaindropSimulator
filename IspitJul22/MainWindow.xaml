<Window x:Class="IspitJul22.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="250" Width="525" ResizeMode="CanMinimize" KeyDown="Window_KeyDown" KeyUp="Window_KeyUp" WindowStartupLocation="CenterScreen">
    <Grid x:Name="MyGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Canvas x:Name="OblakCanvas" Grid.Row="0" >
            <Ellipse Width="30" Height="30" Fill="Blue" x:Name="Oblak" Canvas.Left="234" Canvas.Top="0" HorizontalAlignment="Left" VerticalAlignment="Center"></Ellipse>
        </Canvas>
    </Grid>

</Window>
